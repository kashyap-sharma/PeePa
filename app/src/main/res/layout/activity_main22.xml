<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:openDrawer="start">

    <include
        layout="@layout/app_bar_main22"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <android.support.design.widget.NavigationView
        android:id="@+id/nav_view"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        android:layout_gravity="start">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <include layout="@layout/nav_header_main22"/>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >
                <TextView
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="#EAE7E7"
                    android:layout_alignParentRight="true"/>
                <TextView
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="#EAE7E7"
                    android:layout_alignParentLeft="true"/>
                <com.jlabs.peepaid.customcomponents.TextViewModernM
                    android:layout_width="match_parent"
                    android:background="#EAE7E7"
                    android:text="ACTION"

                    android:textSize="16sp"
                    android:id="@+id/action"
                    android:padding="12dp"
                    android:layout_height="wrap_content" />
                <com.jlabs.peepaid.customcomponents.TextView_Black
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/check"
                    android:id="@+id/toileticon"
                    android:textSize="15sp"
                    android:padding="15dp"
                    android:layout_below="@+id/action"
                    android:layout_marginLeft="25dp"
                    />
                <com.jlabs.peepaid.customcomponents.TextViewModernM
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Toilet"
                    android:id="@+id/toilet"
                    android:textStyle="bold"
                    android:padding="15dp"
                    android:layout_toRightOf="@+id/toileticon"
                    android:textSize="15sp"
                    android:layout_below="@+id/action"

                    />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:id="@+id/v1"
                    android:background="#EAE7E7"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_below="@+id/toilet"
                    />



                <com.jlabs.peepaid.customcomponents.TextView_Black
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/check"
                    android:id="@+id/dustbinicon"
                    android:textSize="15sp"
                    android:padding="15dp"
                    android:layout_below="@+id/v1"
                    android:layout_marginLeft="25dp"
                    />
                <com.jlabs.peepaid.customcomponents.TextViewModernM
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Dustbin"
                    android:id="@+id/dustbin"
                    android:padding="15dp"
                    android:layout_toRightOf="@+id/dustbinicon"
                    android:textSize="15sp"
                    android:layout_below="@+id/v1"

                    />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#EAE7E7"
                    android:id="@+id/v2"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_below="@+id/dustbin"
                    />


                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/redeembutton"
                    android:layout_below="@+id/v2">
                    <com.jlabs.peepaid.customcomponents.TextView_Black
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/check"
                        android:id="@+id/redeemicon"
                        android:textSize="15sp"
                        android:padding="15dp"

                        android:layout_marginLeft="25dp"
                        />
                    <com.jlabs.peepaid.customcomponents.TextViewModernM
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Redeem"
                        android:id="@+id/redeem"
                        android:padding="15dp"
                        android:layout_toRightOf="@+id/redeemicon"
                        android:textSize="15sp"


                        />
                    <com.jlabs.peepaid.customcomponents.TextView_Black
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:padding="15dp"
                        android:text="@string/star_in_circle"/>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#EAE7E7"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:layout_below="@+id/redeemicon"
                        />
                </RelativeLayout>




                <com.jlabs.peepaid.customcomponents.TextView_Black
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/check"
                    android:id="@+id/refericon"
                    android:textSize="15sp"
                    android:padding="15dp"
                    android:layout_below="@+id/redeembutton"
                    android:layout_marginLeft="25dp"
                    />
                <com.jlabs.peepaid.customcomponents.TextViewModernM
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Refer &amp; Earn"
                    android:id="@+id/refer"
                    android:padding="15dp"
                    android:layout_toRightOf="@+id/refericon"
                    android:textSize="15sp"
                    android:layout_below="@+id/redeembutton"

                    />





                <com.jlabs.peepaid.customcomponents.TextViewModernM
                    android:layout_width="match_parent"
                    android:background="#EAE7E7"
                    android:text="INFO"

                    android:textSize="16sp"
                    android:layout_below="@+id/refer"
                    android:id="@+id/info"
                    android:padding="12dp"
                    android:layout_height="wrap_content" />
                <com.jlabs.peepaid.customcomponents.TextView_Black
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/check"
                    android:id="@+id/helpicon"
                    android:textSize="15sp"
                    android:padding="15dp"
                    android:layout_below="@+id/info"
                    android:layout_marginLeft="25dp"
                    />
                <com.jlabs.peepaid.customcomponents.TextViewModernM
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Help n Feedback"
                    android:id="@+id/help"
                    android:padding="15dp"
                    android:layout_toRightOf="@+id/helpicon"
                    android:textSize="15sp"
                    android:layout_below="@+id/info"

                    />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:id="@+id/v4"
                    android:background="#EAE7E7"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_below="@+id/help"
                    />



                <com.jlabs.peepaid.customcomponents.TextView_Black
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/check"
                    android:id="@+id/abouticon"
                    android:textSize="15sp"
                    android:padding="15dp"
                    android:layout_below="@+id/v4"
                    android:layout_marginLeft="25dp"
                    />
                <com.jlabs.peepaid.customcomponents.TextViewModernM
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="About"
                    android:id="@+id/about"
                    android:padding="15dp"
                    android:layout_toRightOf="@+id/abouticon"
                    android:textSize="15sp"
                    android:layout_below="@+id/v4"

                    />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#EAE7E7"
                    android:id="@+id/v5"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_below="@+id/about"
                    />





                <com.jlabs.peepaid.customcomponents.TextView_Black
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/check"
                    android:id="@+id/contacticon"
                    android:textSize="15sp"
                    android:padding="15dp"
                    android:layout_below="@+id/v5"
                    android:layout_marginLeft="25dp"
                    />
                <com.jlabs.peepaid.customcomponents.TextViewModernM
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Contact us"
                    android:id="@+id/contact"
                    android:padding="15dp"
                    android:layout_toRightOf="@+id/contacticon"
                    android:textSize="15sp"
                    android:layout_below="@+id/v5"

                    />



            </RelativeLayout>

        </LinearLayout>

    </android.support.design.widget.NavigationView>


</android.support.v4.widget.DrawerLayout>
